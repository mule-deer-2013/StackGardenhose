<h1>Below are all the questions!</h1>

<ul>
  <% @questions.each do |question| %>
    <li>
      <%= link_to "#{question.title}", question_path(question) %> | <%= render partial: "votes/counter", locals: {votable: question} %> 
    </li>
  <% end %>
</ul>

<%= link_to "Ask a question", new_question_path %>
